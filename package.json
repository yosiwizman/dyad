{
  "name": "abba-ai",
  "productName": "ABBA AI",
  "version": "0.2.8",
  "description": "Free, local, open-source AI app builder",
  "main": ".vite/build/main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/yosiwizman/dyad.git"
  },
  "bugs": {
    "url": "https://github.com/yosiwizman/dyad/issues"
  },
